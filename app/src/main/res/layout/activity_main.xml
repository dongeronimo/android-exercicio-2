<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:text="Numero de execuções"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="18dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/edtVy"/>
    <EditText
            android:layout_width="80dp"
            android:layout_height="43dp"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/edtNumeroDeExecucoes"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="18dp"
            android:layout_marginTop="19dp" app:layout_constraintTop_toBottomOf="@+id/textView3"/>
    <TextView
            android:text="vetor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/edtP0Y" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="18dp"/>
    <EditText
            android:layout_width="80dp"
            android:layout_height="43dp"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/edtVx" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="18dp" app:layout_constraintEnd_toStartOf="@+id/edtVy"
            android:layout_marginEnd="10dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="43dp"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/edtVy" android:layout_marginStart="10dp"
            app:layout_constraintStart_toEndOf="@+id/edtVx" app:layout_constraintEnd_toStartOf="@+id/edtVz"
            android:layout_marginEnd="10dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="43dp"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/edtVz" android:layout_marginStart="10dp"
            app:layout_constraintStart_toEndOf="@+id/edtVy" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="103dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="43dp"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/edtP0Y" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView" app:layout_constraintStart_toEndOf="@+id/edtP0X"
            android:layout_marginStart="10dp" app:layout_constraintEnd_toStartOf="@+id/edtP0Z"
            android:layout_marginEnd="20dp"/>
    <EditText
            android:layout_width="80dp"
            android:layout_height="43dp"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/edtP0X" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="18dp" app:layout_constraintEnd_toStartOf="@+id/edtP0Y"
            android:layout_marginEnd="10dp"/>
    <TextView
            android:text="p0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="18dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="35dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="43dp"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/edtP0Z" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView" app:layout_constraintStart_toEndOf="@+id/edtP0Y"
            android:layout_marginStart="10dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="103dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:id="@+id/txtResultados" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/edtNumeroDeExecucoes"
            android:layout_height="0dp"/>
    <Button
            android:text="Começar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnGo" app:layout_constraintStart_toEndOf="@+id/edtNumeroDeExecucoes"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView3"/>
</androidx.constraintlayout.widget.ConstraintLayout>